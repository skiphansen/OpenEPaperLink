	"contentids": [ 22, 23, 1, 4, 5, 7, 8, 9, 10, 11, 17, 18, 19, 20, 27 ],
	"template": {
		"1": {
			"weekday": [ 320, -5, "Signika-SB.ttf", 100 ],
